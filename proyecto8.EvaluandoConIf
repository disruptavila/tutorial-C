/* Declaracion de archivos de encabezado o headers */
#include<stdio.h>


/* Declaracion de funciones, en este caso, la funcion principal*/
int main(){

	/* Declaracion de variable y asignar valor */
	int numero = 4;

//	/* Condicion: Â¿la variable numero es mayor que 2? */
	if (numero > 5) {
		//Si es verdadero, imprimir en pantalla
		printf("Si, lo es, %d es mayor que 5.", numero);
	} else if(numero < 3){
		//Si es verdadero, imprimir en pantalla
		printf("%d es menor que 3.", numero);

	} else {
		//Si es verdadero, imprimir en pantalla
		printf(" %d no es mayor que 5 ni menor que 3.",numero);
	}
}
