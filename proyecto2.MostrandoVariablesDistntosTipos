#include<stdio.h>

#define PI 3.141592

int edad = 18;
long edadDelUniverso = 137000000;
float altura = 1.78;
double pesoHidrogeno = 0.000000000000000016789;
char letra = 'a';
char diaDeLaSemana[] = "Viernes";

int main() {

	printf("PI = %f .\n", PI);
	printf("Tengo %d anos.\n", edad);
	printf("Universo: %lu.\n", edadDelUniverso);
	printf("Peso Hidrogeno: %.21f.\n", pesoHidrogeno);

}
